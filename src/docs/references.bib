@inproceedings{mathurAnalysis2021,
  title = {Analysis of the {{Knowledge Gain}} and {{Cognitive Load Experienced Due}} to the {{Computer-Aided Instruction}} of {{Additive Manufacturing Processes}}},
  booktitle = {Volume 4: 18th {{International Conference}} on {{Design Education}} ({{DEC}})},
  author = {Mathur, Jayant and Miller, Scarlett R. and Simpson, Timothy W. and Meisel, Nicholas A.},
  date = {2021-08-17},

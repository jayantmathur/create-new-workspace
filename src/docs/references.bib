  shorttitle = {But {{Will}} It {{Print}}?},
  booktitle = {Volume {{2A}}: 45th {{Design Automation Conference}}},
  author = {Prabhu, Rohan and Miller, Scarlett R. and Simpson, Timothy W. and Meisel, Nicholas A.},
  date = {2019-08-18},
  pages = {V02AT03A028},
  publisher = {American Society of Mechanical Engineers},

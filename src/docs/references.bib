@article{armougumVirtual2019,
  title = {Virtual Reality: {{A}} New Method to Investigate Cognitive Load during Navigation},
  shorttitle = {Virtual Reality},
  author = {Armougum, A. and Orriols, E. and Gaston-Bellegarde, A. and Marle, C. Joie-La and Piolino, P.},
